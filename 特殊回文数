n=int(input())
x=[]
for a in range(1,10):
    for b in range(0,10):
        for c in range(0,10):
            if 2*(a+b)+c==n:
                x.append(10000*a+1000*b+100*c+10*b+a)
            if 2*(a+b+c)==n: 
                x.append(100000*a+10000*b+1000*c+100*c+10*b+a)
x.sort()
for i in x:
    print(i)
